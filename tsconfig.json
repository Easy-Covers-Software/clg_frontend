{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/**/*.tsx",
    "app/**/**/**/*.tsx",
    "app/**/**/*.tsx",
    "app/**/**/**/**/*.js",
    "app/**/**/**/**/*.jsx",
    "app/**/**/**/**/*.tsx",
    "app/**/**/**/**/*.ts",
    "app/**/**/*.js",
    "app/api/auth/[...nextauth]/route.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
